{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Grpc": "Debug",
      "Microsoft.Hosting": "Information"
    }
  },
  "GrpcClient": {
    "Address": "http://localhost:8080",
    "TimeoutSeconds": 30,
    "Retry": {
      "MaxAttempts": 5,
      "InitialBackoffMs": 500,
      "MaxBackoffSeconds": 5,
      "BackoffMultiplier": 2
    },
    "Connection": {
      "PooledConnectionLifetimeMinutes": 5,
      "PooledConnectionIdleTimeoutMinutes": 2,
      "KeepAlivePingDelaySeconds": 60,
      "KeepAlivePingTimeoutSeconds": 30,
      "ConnectTimeoutSeconds": 10
    }
  }
}